# GitHub OAuth Configuration
VITE_GITHUB_CLIENT_ID=""                        # Required: OAuth App Client ID from GitHub Developer Settings (alphanumeric characters only)
GITHUB_CLIENT_SECRET=""                         # Required: OAuth App Client Secret from GitHub Developer Settings (40-character hexadecimal)
VITE_GITHUB_OAUTH_SCOPES="read:user,user:email" # Required: OAuth scopes for GitHub authentication (comma-separated)

# GitHub API Configuration
VITE_GITHUB_API_URL="https://api.github.com"    # Optional: GitHub API URL (default: https://api.github.com)
VITE_GITHUB_ORG="near"                          # Required: GitHub organization to track

# NEAR Configuration
VITE_NEAR_BLOCKS_API_KEY=""                     # Required: API key for NEAR Blocks API
VITE_NEAR_INDEXER_URL="https://api.nearblocks.io" # Optional: NEAR Blocks API URL
VITE_NEAR_RPC_URL="https://rpc.mainnet.near.org"  # Optional: NEAR RPC URL for mainnet

# Additional API Endpoints
VITE_DEFI_LLAMA_API="https://api.llama.fi/v2/chains/near" # Optional: DeFi Llama API endpoint

# Development Configuration
NODE_ENV="development"                          # Required: Node environment (development/production)

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
VITE_CLERK_SIGN_IN_URL=https://clerk.yourapp.com/sign-in
VITE_CLERK_SIGN_UP_URL=https://clerk.yourapp.com/sign-up
VITE_CLERK_AFTER_SIGN_IN_URL=https://yourapp.com/
VITE_CLERK_AFTER_SIGN_UP_URL=https://yourapp.com/
VITE_CLERK_OAUTH_CALLBACK_URL=https://clerk.yourapp.com/v1/oauth_callback

# GitHub Configuration
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_GITHUB_API_URL=https://api.github.com
VITE_GITHUB_SCOPES=repo,user
